import { ModuleWithProviders } from '@angular/core';
import { NgxLocalstorageConfiguration } from './interfaces/storage-configuration';
/**
 * Provides the librarys module.
 */
export declare class NgxLocalStorageModule {
    /**
     * Initializes the module for the root module with the given configuration.
     */
    static forRoot(config?: NgxLocalstorageConfiguration): ModuleWithProviders<NgxLocalStorageModule>;
    /**
     * Creates a new instance.
     */
    constructor(parentModule: NgxLocalStorageModule);
}
